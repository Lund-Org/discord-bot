{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run build && npm run start",
    "build": "rm -rf dist && mkdir dist && tsc --project tsconfig.json",
    "start": "supervisor dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "link:dev:build": "cp ormconfig-dev-build.json ormconfig.json",
    "link:dev:run": "cp ormconfig-dev-run.json ormconfig.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "npm run typeorm migration:create -- -n",
    "typeorm:migration:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:migration:up": "npm run typeorm migration:run",
    "typeorm:migration:down": "npm run typeorm migration:revert"
  },
  "author": "Lund",
  "license": "ISC",
  "dependencies": {
    "@twurple/api": "5.0.17",
    "@twurple/auth": "5.0.17",
    "@twurple/pubsub": "5.0.17",
    "@types/canvasjs": "^1.9.6",
    "@types/cron": "1.7.3",
    "@types/express": "^4.17.11",
    "@types/node": "16.11.22",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "canvas": "2.9.0",
    "cron": "1.8.2",
    "discord.js": "13.6.0",
    "dotenv": "14.3.2",
    "ejs": "3.1.6",
    "eslint": "8.7.0",
    "express": "^4.17.1",
    "fs-extra": "10.0.0",
    "mysql2": "2.3.3",
    "reflect-metadata": "0.1.13",
    "supervisor": "0.12.0",
    "ts-node": "10.4.0",
    "typeorm": "^0.2.41",
    "typescript": "4.5.5",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@types/ws": "^7.4.7"
  }
}
